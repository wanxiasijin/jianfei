<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here. 
        -->
    </style>

    <!-- Base application theme. -->
    <style name="SplashActivityTheme" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@drawable/welecom</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/transparent</item>
        <item name="colorPrimaryDark">@color/transparent</item>
        <item name="colorAccent">@color/transparent</item>
    </style>

    <style name="Green_AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="dialogFullscreen">
        <item name="android:windowFullscreen">true</item>
    </style>


    <!-- ActionBar styles -->
    <style name="MyActionBar" parent="@style/AppTheme">
        <item name="android:background">@android:color/white</item>
        <item name="android:textColor">@android:color/black</item>
    </style>


    <style name="MainTabButton">
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:background">@null</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:button">@null</item>
        <item name="android:textColor">@drawable/selector_menu_txt</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="NavPage">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
        <!-- <item name="android:background">@drawable/nav_page</item> -->
        <item name="android:background">@color/navpage</item>
    </style>

    <style name="load_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item><!-- 边框 -->
        <item name="android:windowIsFloating">true</item><!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item><!-- 半透明 -->
        <item name="android:windowNoTitle">true</item><!-- 无标题 -->
        <item name="android:windowBackground">@color/transparent</item><!-- 自己想要的背景 -->
        <item name="android:backgroundDimEnabled">false</item><!-- 模糊 -->
        <item name="android:windowFullscreen">true</item>
    </style>


    <style name="CustomDialog" parent="Theme.AppCompat.Dialog">
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="colorAccent">@color/green_theam</item>
    </style>

    <!--版本更新-->
    <style name="versionUpdateDialog" parent="android:Theme.Dialog">
        <!--背景颜色及透明程度-->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--是否有标题 -->
        <item name="android:windowNoTitle">false</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsFloating">true</item>
        <!--是否模糊-->
        <item name="android:backgroundDimEnabled">true</item>
    </style>


    <style name="Widget.App.ProgressBar" parent="Widget.AppCompat.ProgressBar">
        <!--<item name="android:indeterminateDrawable">@drawable/progressbar_loading</item>-->
    </style>

    <style name="Widget.App.ProgressBar.Small" parent="Widget.AppCompat.ProgressBar">
        <!--<item name="android:indeterminateDrawable">@drawable/progressbar_loading_small</item>-->
    </style>


    <style name="Transparent_Dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">false</item>
        <!-- 模糊 -->
    </style>

    <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_style</item>
    </style>

    <style name="login">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textSize">@dimen/text_16</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/bg_coners_tract</item>
    </style>

    <style name="reset_password">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:textColor">@color/text_63</item>
        <item name="android:textSize">@dimen/text_16</item>
        <item name="android:textColorHint">@color/text_b2</item>
        <item name="android:background">@drawable/bg_coners_white</item>
    </style>

    <style name="forget_password_edit">

        <item name="android:layout_height">48dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textColorHint">@color/text_b2</item>
        <item name="android:textColor">@color/text_63</item>
        <item name="android:textSize">@dimen/text_16</item>
        <item name="android:background">@drawable/bg_coners_white</item>
    </style>

    <style name="btn_submit">
        <item name="android:background">@drawable/btn_theme_selector</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_16</item>
    </style>

    <style name="TabLayoutTextStyle">
        <item name="android:textSize">@dimen/text_16</item>
    </style>

    <style name="TextLabelgrey33">
        <item name="android:textColor">@color/text_39</item>
        <item name="android:textSize">@dimen/text_16</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="TextTitle">
        <item name="android:textColor">@color/text_grey_black</item>
        <item name="android:textSize">@dimen/text_17</item>
    </style>

    <style name="Text15Labelgrey33">
        <item name="android:textColor">@color/text_grey_black</item>
        <item name="android:textSize">@dimen/text_15</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="Text15Labelgrey331">
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">@dimen/text_15</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="ButtonDialogLeft">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_16</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/btn_dialog_left_selector</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
    </style>

    <style name="ButtonDialogRight">
        <item name="android:textColor">@color/bg_theme</item>
        <item name="android:textSize">@dimen/text_16</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/btn_dialog_right_selector</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
    </style>


    <style name="start_and_end">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_14</item>

    </style>

    <style name="text_mid_orange">
        <item name="android:textColor">@color/text_39</item>
        <item name="android:textSize">@dimen/text_16</item>
    </style>

    <style name="edit_background">
        <item name="android:textColor">@color/text_63</item>
        <item name="android:textSize">@dimen/text_14</item>
        <item name="android:background">@drawable/bg_edit</item>
        <item name="android:maxLines">2</item>
        <item name="android:padding">15dp</item>
    </style>


    <style name="btn_dialog_cancel">
        <item name="android:background">@drawable/btn_dialog_selector</item>
        <item name="android:textColor">@color/text_63</item>
        <item name="android:textSize">@dimen/text_16</item>
    </style>


    <style name="btn_dialog_ok">
        <item name="android:background">@drawable/btn_dialog_selector</item>
        <item name="android:textColor">@color/bg_theme</item>
        <item name="android:textSize">@dimen/text_16</item>
    </style>

    <!--######################################REBUILD######################################-->
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="actionMenuTextAppearance">@style/MenuTextAppearance</item>
        <item name="android:actionMenuTextAppearance">@style/MenuTextAppearance</item>
    </style>

    <style name="MenuTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Menu">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TranslucentStatusBar" parent="android:Widget">
        <item name="android:visibility">gone</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light">
        <item name="actionOverflowMenuStyle">@style/OverflowMenuStyle</item>
    </style>

    <style name="OverflowMenuStyle" parent="Widget.AppCompat.Light.PopupMenu.Overflow" />

    <style name="TitleTextAppearance" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:maxLines">1</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="AppTheme.Translucent" parent="AppTheme.TranslucentStatus">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppAlertDialogStyle" parent="@style/Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/green_theam</item>
        <item name="buttonBarNeutralButtonStyle">@style/AlertDialog_Neutral_Button_Style</item>
    </style>

    <style name="FPopupMenuStyle" parent="@style/Widget.AppCompat.Light.PopupMenu">
        <item name="overlapAnchor">true</item>
        <item name="android:dropDownVerticalOffset">0dp</item>
        <item name="android:dropDownHorizontalOffset">0dp</item>
    </style>

    <style name="FPopupMenuTextAppearanceLarge" parent="@style/TextAppearance.AppCompat.Light.Widget.PopupMenu.Large">
        <item name="android:textColor">@color/sd_text_value</item>
    </style>

    <style name="FPopupMenuTextAppearanceSmall" parent="@style/TextAppearance.AppCompat.Light.Widget.PopupMenu.Small">
        <item name="android:textColor">@color/sd_text_value</item>
    </style>

    <style name="AlertDialog_Neutral_Button_Style">
        <item name="android:textColor">@color/sd_text_value</item>
        <item name="android:background">@null</item>
    </style>


    <style name="fileProgressBarStyle" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:maxHeight">50dp</item>
        <item name="android:minHeight">10dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">
            @android:drawable/progress_indeterminate_horizontal
        </item>
    </style>

    <style name="main_menu_photo_anim">
        <item name="android:windowEnterAnimation">@anim/photo_dialog_in_anim</item>
        <item name="android:windowExitAnimation">@anim/photo_dialog_out_anim</item>
    </style>

    <style name="popupWindowAnimRight">
        <item name="android:windowEnterAnimation">@anim/page_anima_right_in</item>
    </style>

    <style name="register_hint_Style1">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/black_sd2</item>
    </style>

    <style name="register_hint_Style2">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black_sd2</item>
    </style>

    <style name="crowd_item_progressBar">
        <item name="android:indeterminateOnly">false</item>   
        <item name="android:minHeight">10dp</item>       
        <item name="android:maxHeight">10dp</item>
    </style>

</resources>
